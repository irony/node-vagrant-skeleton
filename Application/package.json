{
    "name" : "node-vagrant-skeleton",
    "version" : "0.1.2",
    "scripts": {
        "start": "node_modules/forever/bin/./forever start ./Application/run.js",
        "stop": "node_modules/forever/bin/./forever stop /Application/run.js",
        "restart": "node_modules/forever/bin/./forever restart /Application/run.js",
        "test" : "node tests/run-all.js;"
    },
    "dependencies" : {
        "forever" : "*",
        "colors" : "*",
        "jslint": "*"
    }
}